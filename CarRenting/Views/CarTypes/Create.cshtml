@model CarRenting.Models.CarType

@{
    ViewBag.Title = "Criar categoria";
}
<div class="boxshadow log-headers-h2 col-md-8 offset-md-2">
    <h2>@ViewBag.Title</h2>
</div>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="jumbotron boxshadow col-md-8 offset-md-2">
 
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group col-md-10 offset-1">
            @Html.LabelFor(model => model.Type, htmlAttributes: new { @class = "col-form-label" })
            @Html.EditorFor(model => model.Type, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.Type, "", new { @class = "text-danger" })
        </div>
        <br/>
        <div class="form-group text-center">
            @Html.ActionLink("Voltar", "Index", null, new{@class="btn-danger btn"})
            <input type="submit" value="Criar" class="btn btn-primary" />
        </div>
    </div>
}

<div>
    
</div>
